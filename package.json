    "gatsby-plugin-mdx": "^1.0.64",
    "gatsby-plugin-react-leaflet": "^2.0.13",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-source-apiserver": "2.1.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-opencage-geocoder": "^1.0.0",
    "gatsby-transformer-remark": "^2.2.0",
    "gatsby-transformer-sharp": "^2.1.1",
    "leaflet": "^1.6.0",
    "opencage-api-client": "^0.7.1",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-leaflet": "^2.7.0",
